set sendmail = "/usr/local/bin/msmtp"
set alias_file = ~/Mail/alias_file
source $alias_file

# email headers
set header_cache=~/Mail/cache/headers
set message_cachedir=~/Mail/cache/bodies

# Other config files
source ~/Mail/config/common.muttrc
source ~/Mail/config/keybindings.muttrc
source ~/Mail/config/colors.muttrc

# default email
source ~/Mail/config/accounts/gmail_sidcha.dev.muttrc
